{
    "name": "crumbls/pipeline",
    "description": "State-aware pipeline implementation for Laravel with job-based execution support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chase Miller",
            "email": "chase@crumbls.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Crumbls\\Pipeline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crumbls\\Pipeline\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crumbls\\Pipeline\\PipelineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
